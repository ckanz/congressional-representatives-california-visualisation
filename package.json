{
  "name": "congressional-representatives-california-visualisation",
  "version": "0.2.0",
  "description": "This purpose of this project is to create a web application to monitor the behaviour of Californian Members of Congress using the [ProPublica Congress API](https://projects.propublica.org/api-docs/congress-api/).",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "serve": "webpack-dev-server --config webpack.config.js --mode development",
    "watch:test": "karma start --auto-watch --no-single-run --reporters dots",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckanz/congressional-representatives-california-visualisation.git"
  },
